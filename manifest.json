{
  "manifest_version": 2,
  "name": "New Tab",
  "version": "0.1",
"content_scripts": [
  {
    "matches": [
      "*://*/_/chrome/newtab*"
    ],
    "js": ["content.js"]
  }
],
 "chrome_url_overrides": {
    "newtab": "html/newTab.html"
  },
"browser_action": {
  "default_icon": "icon.png"
}
  }

